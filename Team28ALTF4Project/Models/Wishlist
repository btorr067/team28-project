// Book.cs
public class Book
{
    public int Id { get; set; }
    public string Title { get; set; }
    public string Author { get; set; }
}

// Wishlist.cs
public class Wishlist
{
    public int Id { get; set; }
    public string UserId { get; set; }
    public string Name { get; set; }
    public List<Book> Books { get; set; }
}
